{
    "name": "positibe/sylius-fee-bundle",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin", "symfony", "e-commerce", "price", "symfony-bundle"],
    "description": "Fee Bundle for Sylius Project.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "sylius/core-bundle": "^1.4",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Positibe\\Sylius\\FeePlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Positibe\\Sylius\\FeePlugin\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
